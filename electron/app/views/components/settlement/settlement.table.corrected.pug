.sy-table__container(ng-if="$ctrl.listErrorsContributors")
  .sy__title-table(ng-bind="'SETTLEMENT.TABLES.CORRECTED_AUT' | translate")
  error-contributors-aut(selected-item-aut="$ctrl.selectedItem" errors-filter="$ctrl.filterSugeridos" data="$ctrl.listErrorsContributors" object-filter="{autocorregible:true}" style-error="false")
  .sy__button-container.sett__buttons
    div
      .sy__button--blue(ng-class="{}" ng-click="$ctrl.validateSelected()")
        i.mdi.mdi-pencil
        span(ng-bind="'SETTLEMENT.ACTIONS.CORRECTED' | translate")
    div
      .sy__button--blue(ng-class="{}" ng-click="$ctrl.actionExportToCsvSug()")
        i.mdi.mdi-content-save
        span(ng-bind="'SETTLEMENT.ACTIONS.EXPORT_CSV' | translate")
.sy-table__container(ng-if="$ctrl.listErrorsContributors")
  .sy__title-table(ng-bind="'SETTLEMENT.TABLES.CORRECTED' | translate")
  error-contributors-sug(data="$ctrl.listErrorsContributors" errors-filter="$ctrl.filterCorregidos" object-filter="{autocorregible:true}" style-error="false")
  .sy__button-container.sett__buttons
    div
      .sy__button--blue(ng-class="{}" ng-click="$ctrl.actionExportToCsvCorr()")
        i.mdi.mdi-content-save
        span(ng-bind="'SETTLEMENT.ACTIONS.EXPORT_CSV' | translate")
